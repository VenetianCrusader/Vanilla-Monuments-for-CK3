		monthly_income = 5
	}

	county_modifier = {
		tax_mult = 0.4
		development_growth_factor = 0.4
		development_growth = 0.2
	}
	character_modifier = {
		monthly_prestige = 1
		monthly_dynasty_prestige_mult = 0.1
		monthly_county_control_change_factor = 0.2

	}
