grand_cathedrial = {
	construction_time = very_slow_construction_time
	
	is_enabled = {
		has_variable = variable_grand_cathedral
	}
	
	province_modifier = {
		monthly_income = 3
	}

	county_modifier = {
		tax_mult = 0.4
		development_growth_factor = 0.4
		development_growth = 0.2
	}
	character_modifier = {
		learning = 1
    monthly_piety = 0.5
		monthly_dynasty_prestige_mult = 0.1
		holy_order_hire_cost_mult = -0.3
		clergy_opinion = 10
		monthly_county_control_change_factor = 0.1
	}

	type_icon = "icon_structure_cologne_cathedral.dds"
	
	ai_value = {
		base = 100
	}
	
	#type = special
}

